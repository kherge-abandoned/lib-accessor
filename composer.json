{
    "name": "phine/accessor",
    "description": "A PHP library for simplifying the use of accessors.",
    "keywords": ["accessors", "getter", "setter"],
    "homepage": "https://github.com/phine/lib-accessor",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Herrera",
            "email": "kevin@herrera.io",
            "homepage": "http://kevin.herrera.io"
        }
    ],
    "support": {
        "issues": "https://github.com/phine/lib-accessor/issues"
    },
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "league/phpunit-coverage-listener": "~1.0",
        "phine/test": "~1.0"
    },
    "autoload": {
        "psr-0": {
            "Phine\\Accessor": "src/lib"
        }
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "src/vendors"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
